<%= render "layouts/errors", :object => @wine %>

<%= form_for(@wine) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <%= f.label :grapes %>
  <%= f.text_field :grapes %>
  <%= f.label :label %>
  <%= f.text_field :label %>
  <%= f.label :winery %>
  <%= f.text_field :winery %>
  
  <%= f.submit %>
<% end %>
