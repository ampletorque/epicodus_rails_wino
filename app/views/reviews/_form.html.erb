<%= render('layouts/errors', :object => @review) %>

<%= form_for [@wine, @review] do |f| %>
  <%= f.label :user %>
  <%= f.text_field :user %>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <%= f.label :description %>
  <%= f.text_field :description %>

  <p><%= f.submit %></p>
<% end %>
